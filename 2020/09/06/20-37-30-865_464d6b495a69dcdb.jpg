            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" data-ng-show="btnAlertCancelar" data-ng-click="inicializaModalAlerta()"><span class="glyphicon glyphicon-remove"></span>Cancelar</button>
                <button type="button" class="btn btn-primary" data-ng-click="guardar()" data-ng-show="btnAlertGuardar"><span class="glyphicon glyphicon-bell"></span>Recibir alerta</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal" data-ng-show="btnAlertCerrar" data-ng-click="inicializaModalAlerta()"><span class="glyphicon glyphicon-bell"></span>Cerrar</button>
            </div>

            <script>
                var IdTipoOportunidadVoluntariado = 2;
                function formController($scope, $http) {
                    $scope.inicializaModalAlerta = function () {
                        $scope.divOkAlerta = false;
                        $scope.divErrorAlerta = false;
                        $scope.divLoading = false;
                        $scope.formAlertaShow = true;
                        $scope.btnAlertCancelar = true;
                        $scope.btnAlertGuardar = true;
                        $scope.btnAlertCerrar = false;
                        $scope.valMailVacio = false;
                        $scope.valMailnoEsMail = false;
                        $scope.valMailNoCoincide = false;
                        $scope.valPoliticaNoAceptada = false;
                        $scope.msgCreada = false;
                        formAlerta.email.value = "";
                        formAlerta.email2.value = "";
                        formAlerta.chkPol.checked = false;
                    };
                    $scope.guardar = function () {

                        $scope.formAlertaShow = false;
                        $scope.divLoading = true;

                        var query = window.location.href.slice(window.location.href.indexOf('?') + 1);
                        var data = { "model": { "Email": formAlerta.email.value, "Email2": formAlerta.email2.value, "PoliticaAceptada": formAlerta.chkPol.checked, "Parametros": query, "Descripcion": formAlerta.descripcion.value,"IdTipoOportunidadVoluntariado": IdTipoOportunidadVoluntariado} };
                        $http.post("/oportunidades/buscador/default.aspx/Crear", JSON.stringify(data),
                                           { headers: { 'Content-Type': 'application/json' } }
                                )
                                .success(function (response) {
                                    $scope.divLoading = false;
                                    var model = JSON.parse(response.d);

                                    $scope.valMailVacio = model.MailVacio;
                                    $scope.valMailnoEsMail = model.MailNoEsMail;
                                    $scope.valMailNoCoincide = model.MailNoCoincide;
                                    $scope.valPoliticaNoAceptada = model.PoliticaNoAceptada;
                                    $scope.msgCreada = model.Creada;
                                    $scope.btnAlertCancelar = !model.Creada;
                                    $scope.btnAlertGuardar = !model.Creada;
                                    $scope.btnAlertCerrar = model.Creada;
                                    if (model.Creada) {
                                        $scope.divOkAlerta = true;
                                        $scope.divErrorAlerta = false;
                                        $scope.formAlertaShow = false;
                                    }
                                    else {
                                        $scope.divOkAlerta = false;
                                        $scope.divErrorAlerta = true;
                                        $scope.formAlertaShow = true;
                                    }
                                })
                                .error(function (data, status, headers, config) {
                                    $scope.formAlertaShow = true;
                                    $scope.divLoading = false;
                                    alert("Ocurrió un error. Inténtalo de nuevo.");
                                }
                               );
                    };

                    $scope.inicializaModalAlerta();
                }
                function descripcionController($scope) {
                    if (filtroData) {
                        $scope.descripcion = filtroData.Descripcion;
                    }
                }
            </script>
